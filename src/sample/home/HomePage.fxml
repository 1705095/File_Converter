<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.home.HomePage">
   <children>
      <ImageView fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true">
         <image>
            <Image url="@../../../res/background.jpg" />
         </image>
      </ImageView>
      <Label fx:id="nameLabel" layoutX="43.0" layoutY="45.0" prefHeight="56.0" prefWidth="339.0" text="File Converter" textFill="WHITE">
         <font>
            <Font size="43.0" />
         </font>
      </Label>
      <Button fx:id="pdfdoc" layoutX="43.0" layoutY="175.0" mnemonicParsing="false" onAction="#PDFtoDOC" prefHeight="31.0" prefWidth="266.0" stylesheets="@../Style.css" text="PDF to DOCX" />
      <Button fx:id="docPDF" layoutX="43.0" layoutY="254.0" mnemonicParsing="false" onAction="#DOCtoPDF" prefHeight="31.0" prefWidth="266.0" stylesheets="@../Style.css" text="DOCX to PDF" />
      <Button fx:id="mergepdf" layoutX="43.0" layoutY="329.0" mnemonicParsing="false" onAction="#MergePdf" prefHeight="31.0" prefWidth="266.0" stylesheets="@../Style.css" text="Merge PDF" />
      <Button fx:id="mergedoc" layoutX="43.0" layoutY="410.0" mnemonicParsing="false" onAction="#MergeDocx" prefHeight="31.0" prefWidth="266.0" stylesheets="@../Style.css" text="Merge DOCX" />
      <Button fx:id="image" layoutX="43.0" layoutY="569.0" mnemonicParsing="false" onAction="#ImageResizeButton" prefHeight="31.0" prefWidth="266.0" stylesheets="@../Style.css" text="Image Resize" />
